<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <!--音乐界面顶部的文字大小-->
    <style name="MusicTabTextStyle" tools:ignore="ResourceName">
        <item name="android:textSize">28sp</item>
    </style>

    <style name="FirstTextViewItemStyle" tools:ignore="ResourceName">
        <item name="android:maxWidth">350dp</item>
        <item name="android:layout_width">350dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:ellipsize">end</item>
        <item name="android:singleLine">true</item>
        <item name="android:gravity">start|center_vertical</item>
        <item name="android:textColor">@color/usb_music_white</item>
        <item name="android:textSize">36sp</item>
    </style>

</resources>